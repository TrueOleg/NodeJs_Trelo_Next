{
  "name": "NodeJs_Starter",
  "version": "1.0.0",
  "description": "Demo project",
  "main": "index.js",
  "repository": "https://github.com/TrueOleg/NodeJs_Starter.git",
  "author": "Oleg Zaichkin <trueolegzaichkin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rimraf ./dist && babel -d ./dist ./src -s && node dist/main.js && npm run dbcreate",
    "dev": "node dev.js",
    "lint:js": "eslint ./src",
    "lint-staged": "lint-staged",
    "dbcreate": "node_modules/.bin/sequelize db:create development && node_modules/.bin/sequelize db:migrate",
    "testdbcreate": "node_modules/.bin/sequelize db:create --env test && node_modules/.bin/sequelize db:migrate --env test",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --compilers js:babel-core/register test -w --recursive"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "pre-commit": "^1.2.2",
    "sequelize-cli": "^4.1.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "node": true
    }
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "lodash": "^4.17.10",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "rimraf": "^2.6.2",
    "sequelize": "^4.38.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}